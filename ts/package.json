{
  "name": "typescript-node-project",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build:example": "rm -rf dist && tsup examples/main.ts --format esm --minify",
    "watch": "tsup examples/main.ts --watch --format esm",
    "check": "tsc --build",
    "lint": "eslint examples/ tests/",
    "lint:fix": "eslint examples/ tests/ --fix",
    "test": "vitest run --test-timeout=15000",
    "test:watch": "vitest --test-timeout=15000",
    "gen:example": "cd examples/gen && cargo run -- --clean",
    "gen:tests": "cd tests/gen && cargo run -- --clean",
    "gen:all": "pnpm run -r gen:example & pnpm run -r gen:tests & wait && pnpm run lint:fix"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@mysten/sui": "^1.45.0",
    "commander": "^12.1.0",
    "tsup": "^8.2.4"
  }
}
